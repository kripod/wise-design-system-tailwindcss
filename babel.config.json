{
  "presets": [
    ["@babel/preset-env", { "bugfixes": true }],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "importSource": "jsx-scoped-css-reset"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["babel-plugin-optimize-clsx", { "functionNames": ["clsx"] }]
  ]
}
