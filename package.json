{
  "name": "@kripod/wise-design-system-tailwindcss",
  "version": "0.5.6",
  "description": "Experimental web implementation of Wise Design System, based upon Tailwind CSS.",
  "repository": "kripod/wise-design-system-tailwindcss",
  "homepage": "https://github.com/kripod/wise-design-system-tailwindcss#readme",
  "bugs": "https://github.com/kripod/wise-design-system-tailwindcss/issues",
  "author": "Kristóf Poduszló (https://www.kripod.dev/)",
  "license": "MIT",
  "packageManager": "pnpm@8.15.3",
  "main": "./dist/mod.js",
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    },
    "./dist/preflight.css": "./dist/preflight.css",
    "./dist/pregenerated.css": "./dist/pregenerated.css",
    "./tailwind-base": {
      "types": "./dist/tailwind-base.d.ts",
      "import": "./dist/tailwind-base.mjs",
      "require": "./dist/tailwind-base.js"
    }
  },
  "types": "./dist/mod.d.ts",
  "files": [
    "codemods/",
    "dist/",
    "src/",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "design-system",
    "tailwindcss"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "storybook dev --port 6006",
    "build": "npm-run-all --parallel build:*",
    "build:lib": "npm-run-all --parallel build:lib:css build:lib:js",
    "build:lib:watch": "npm-run-all --parallel build:lib:*:watch",
    "build:lib:css": "postcss src/preflight.css src/pregenerated.css --base src/ --dir dist/",
    "build:lib:css:watch": "pnpm run build:lib:css --watch",
    "build:lib:js": "rollup --config --sourcemap",
    "build:lib:js:watch": "pnpm run build:lib:js --watch",
    "build:stories": "storybook build",
    "clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
    "lint": "npm-run-all --parallel lint:js+ts lint:format lint:types",
    "lint:js+ts": "eslint --ext .js,.jsx,.mjs,.ts,.tsx --ignore-path .gitignore .",
    "lint:format": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "npm-run-all --serial lint:fix:*",
    "lint:fix:js+ts": "pnpm run lint:js+ts --fix",
    "lint:fix:format": "prettier --write ."
  },
  "peerDependencies": {
    "@tailwindcss/container-queries": "^0.1",
    "@transferwise/icons": "^3.1",
    "css-homogenizer": "^3.2.1",
    "react": "18",
    "react-dom": "18",
    "tailwindcss": "^3.4",
    "typescript": ">=4.5"
  },
  "peerDependenciesMeta": {
    "@tailwindcss/container-queries": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@floating-ui/react": "^0.26.9",
    "@headlessui/react": "^1.7.18",
    "@react-aria/overlays": "^3.21.1",
    "clsx": "^2.1.0",
    "merge-props": "^6.0.0",
    "modern-normalize": "^2.0.0",
    "react-merge-refs": "^2.1.1",
    "ts-essentials": "^9.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/plugin-transform-runtime": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@csstools/postcss-rebase-url": "1.0.6",
    "@fontsource-variable/inter": "5.0.16",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@storybook/addon-a11y": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/addon-themes": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.17",
    "@storybook/test": "7.6.17",
    "@tailwindcss/container-queries": "0.1.1",
    "@transferwise/icons": "3.13.0",
    "@tsconfig/recommended": "1.0.3",
    "@types/babel__core": "7.20.5",
    "@types/jscodeshift": "0.11.11",
    "@types/node": "20.11.20",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "4.2.1",
    "@wise/eslint-config": "9.7.2",
    "babel-plugin-optimize-clsx": "2.6.2",
    "css-homogenizer": "3.2.1",
    "eslint": "8.56.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-tailwindcss": "3.14.3",
    "husky": "9.0.11",
    "jscodeshift": "0.15.2",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.35",
    "postcss-cli": "11.0.0",
    "postcss-import": "16.0.1",
    "postcss-preset-env": "9.4.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "4.12.0",
    "storybook": "7.6.17",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "*.css"
  ]
}
