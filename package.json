{
  "name": "@kripod/wise-design-system-tailwindcss",
  "version": "0.6.1",
  "description": "Experimental web implementation of Wise Design System, based upon Tailwind CSS.",
  "keywords": [
    "design-system",
    "tailwindcss"
  ],
  "homepage": "https://github.com/kripod/wise-design-system-tailwindcss#readme",
  "bugs": "https://github.com/kripod/wise-design-system-tailwindcss/issues",
  "repository": "kripod/wise-design-system-tailwindcss",
  "license": "MIT",
  "author": "Kristóf Poduszló (https://www.kripod.dev/)",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/preflight.css": "./dist/preflight.css",
    "./dist/pregenerated.css": "./dist/pregenerated.css",
    "./tailwind-base": {
      "types": "./dist/tailwind-base.d.ts",
      "import": "./dist/tailwind-base.mjs",
      "require": "./dist/tailwind-base.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "codemods/",
    "dist/",
    "src/"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "storybook dev --port 6006",
    "build": "pnpm run \"/^build:[^:]+$/\"",
    "build:lib": "pnpm run \"/^build:lib:.+(?<!:watch)$/\"",
    "build:lib:watch": "pnpm run \"/^build:lib:.+:watch/\"",
    "build:lib:css": "postcss src/preflight.css src/pregenerated.css --base src/ --dir dist/",
    "build:lib:css:watch": "pnpm run build:lib:css --watch",
    "build:lib:js": "vite build",
    "build:lib:js:watch": "pnpm run build:lib:js --watch",
    "build:stories": "storybook build",
    "clean": "git clean --interactive -dx --exclude node_modules/ --exclude .husky/",
    "lint": "pnpm run \"/^lint:(?!fix)/\"",
    "lint:js+ts": "eslint --ext .js,.jsx,.mjs,.ts,.tsx,.mts --ignore-path .gitignore .",
    "lint:format": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "pnpm run \"/^lint:fix:/\"",
    "lint:fix:js+ts": "pnpm run lint:js+ts --fix",
    "lint:fix:format": "prettier --write ."
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.16",
    "@headlessui/react": "^2.0.4",
    "@react-aria/overlays": "^3.22.1",
    "clsx": "^2.1.1",
    "merge-props": "^6.0.0",
    "modern-normalize": "^2.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.3",
    "@fontsource-variable/inter": "5.0.18",
    "@storybook/addon-a11y": "8.1.3",
    "@storybook/addon-essentials": "8.1.3",
    "@storybook/addon-interactions": "8.1.3",
    "@storybook/addon-links": "8.1.3",
    "@storybook/addon-themes": "8.1.3",
    "@storybook/blocks": "8.1.3",
    "@storybook/react": "8.1.3",
    "@storybook/react-vite": "8.1.3",
    "@storybook/test": "8.1.3",
    "@tailwindcss/container-queries": "0.1.1",
    "@total-typescript/tsconfig": "1.0.4",
    "@transferwise/icons": "3.13.0",
    "@types/jscodeshift": "0.11.11",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@wise/eslint-config": "11.2.0",
    "css-homogenizer": "4.0.0",
    "eslint": "8.57.0",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-tailwindcss": "3.17.0",
    "husky": "9.0.11",
    "jscodeshift": "0.15.2",
    "nano-staged": "0.8.0",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0",
    "postcss-preset-env": "9.5.14",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.1.3",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-dts": "3.9.1"
  },
  "peerDependencies": {
    "@tailwindcss/container-queries": "^0.1",
    "@transferwise/icons": "^3.1",
    "css-homogenizer": "^3.2.1",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.4",
    "typescript": ">=5.4"
  },
  "peerDependenciesMeta": {
    "@tailwindcss/container-queries": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.1.2",
  "publishConfig": {
    "access": "public"
  }
}
